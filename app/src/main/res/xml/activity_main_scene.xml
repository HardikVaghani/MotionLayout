<!--               motion:motionTarget="@+id/image_view"-->
<!--               motion:framePosition="1"-->
<!--               motion:wavePeriod="1"-->
<!--               motion:waveShape="sawtooth"-->
<!--               android:rotation="180"-->
<!--               android:alpha="1"-->
<!--               motion:wavePhase="1"-->
<!--               motion:waveOffset="1" />-->
           <KeyCycle
               motion:motionTarget="@+id/image_view"
               motion:framePosition="1"
               motion:wavePeriod="0.5"
               motion:waveShape="sawtooth"
               android:rotation="180"
               />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>