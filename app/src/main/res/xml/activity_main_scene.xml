<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="3000">
        <KeyFrameSet></KeyFrameSet>
        <OnSwipe
            motion:dragDirection="dragUp"
            motion:touchAnchorId="@+id/icon_file"
            motion:touchAnchorSide="top" />
<!--        <OnClick />-->
        <OnClick motion:targetId="@+id/icon_file" />
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/icon_01"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_02"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_02"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_03"
            motion:layout_constraintStart_toEndOf="@id/icon_01"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_03"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/icon_02"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_04"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_05"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/icon_01" />
        <Constraint
            android:id="@+id/icon_05"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_06"
            motion:layout_constraintStart_toEndOf="@id/icon_04"
            motion:layout_constraintTop_toBottomOf="@id/icon_01" />
        <Constraint
            android:id="@+id/icon_06"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/icon_05"
            motion:layout_constraintTop_toBottomOf="@id/icon_01" />
        <Constraint
            android:id="@+id/icon_07"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_08"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/icon_04" />
        <Constraint
            android:id="@+id/icon_08"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_09"
            motion:layout_constraintStart_toEndOf="@id/icon_07"
            motion:layout_constraintTop_toBottomOf="@id/icon_04" />
        <Constraint
            android:id="@+id/icon_09"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/icon_08"
            motion:layout_constraintTop_toBottomOf="@id/icon_04" />
        <Constraint
            android:id="@+id/icon_file"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginBottom="20dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/icon_01"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_02"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_02"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_03"
            motion:layout_constraintStart_toEndOf="@id/icon_01"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_03"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_04"
            motion:layout_constraintStart_toEndOf="@id/icon_02"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_04"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_05"
            motion:layout_constraintStart_toEndOf="@id/icon_03"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_05"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@id/icon_06"
            motion:layout_constraintStart_toEndOf="@id/icon_04"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_06"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/icon_05"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/icon_07"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            motion:layout_constraintEnd_toStartOf="@id/icon_08"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/icon_04" />
        <Constraint
            android:id="@+id/icon_08"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            motion:layout_constraintEnd_toStartOf="@id/icon_09"
            motion:layout_constraintStart_toEndOf="@id/icon_07"
            motion:layout_constraintTop_toBottomOf="@id/icon_04" />
        <Constraint
            android:id="@+id/icon_09"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:visibility="gone"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/icon_08"
            motion:layout_constraintTop_toBottomOf="@id/icon_04" />
        <Constraint
            android:id="@+id/icon_file"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginBottom="28dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/icon_07" />
    </ConstraintSet>
</MotionScene>